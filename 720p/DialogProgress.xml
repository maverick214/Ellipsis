				<align>center</align>
				<texturenofocus border="8">controls\button_red_nf.png</texturenofocus>
				<texturefocus border="8">controls\button_red_f.png</texturefocus>
				<textcolor>TextNegative</textcolor>
				<focusedcolor>Text</focusedcolor>
				<onleft>10</onleft>
				<onright>10</onright>
				<onup>10</onup>

	<include name="Now_Playing_Widget">
		<control type="group">
			<visible>!Skin.HasSetting(Disable_Now_Playing_Widget)</visible>
			<control type="group">
				<posx>1140</posx>
				<posy>610</posy>
				<width>130</width>
				<height>130</height>
				<visible>Player.HasAudio</visible>
				<control type="image">
					<posx>28</posx>
					<posy>0</posy>
					<width>90</width>
					<height>90</height>
					<texture>record.png</texture>
				</control>
				<control type="image">
					<posx>0</posx>
					<posy>0</posy>
					<width>90</width>
					<height>90</height>
					<aspectratio align="center" scalediffuse="false">scale</aspectratio>
					<texture diffuse="record-diffuse.png">$INFO[MusicPlayer.Cover]</texture> 
				</control>
				<control type="image">
					<posx>0</posx>
					<posy>0</posy>
					<width>90</width>
					<height>90</height>
					<texture>record-front.png</texture>
				</control>	
				<control type="image">
					<posx>28</posx>
					<posy>83</posy>
					<width>90</width>
					<height>90</height>
					<texture flipy="true" diffuse="fade.png">record.png</texture>
				</control>
				<control type="image">
					<posx>0</posx>
					<posy>83</posy>
					<width>90</width>
					<height>90</height>
					<aspectratio align="center" scalediffuse="false">scale</aspectratio>
